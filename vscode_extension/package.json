{
  "name": "jails",
  "displayName": "Jai Language Server",
  "description": "Language Server for Jai",
  "version": "0.0.4",
  "publisher": "SogoCZE",
  "author": "SogoCZE",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:jai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SogoCZE/jai_language_server"
  },
  "scripts": {
    "compile": "tsc -b",
    "watch": "tsc --watch",
    "pack:unix": "cp ../bin/jails ./out/ && vsce package",
    "pack:windows": "copy ..\\bin\\jails.exe .\\out\\ && vsce package"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "dependencies": {
    "vscode-languageclient": "8.0.2"
  },
  "devDependencies": {
    "typescript": "4.9.4",
    "@types/vscode": "1.74.0",
    "@types/node": "18.11.18"
  },
  "keywords": [
    "Jai"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jails.restart",
        "title": "Restart",
        "category": "Jails"
      },
      {
        "command": "jails.start",
        "title": "Start",
        "category": "Jails"
      },
      {
        "command": "jails.stop",
        "title": "Stop",
        "category": "Jails"
      }
    ],
    "languages": [
      {
        "id": "jai",
        "aliases": [
          "Jai",
          "jai"
        ],
        "extensions": [
          ".jai"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jai",
        "scopeName": "source.jai",
        "path": "./syntaxes/jai.tmLanguage.json"
      }
    ]
  }
}